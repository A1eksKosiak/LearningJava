package com.gmail.a1ekskosyak;

public class Main {

	public static void main(String[] args) {
		double a;
		double b;
		double c;
		boolean triangleExists;
		a = 0.4;
		b = 1.2;
		c = 0.7;
		if ((a + b >= c) && (a + c >= b) && (b + c >= a)) {
			triangleExists = true;
		}else {
			triangleExists = false;
		}
		System.out.println(triangleExists);
	
	}

}
